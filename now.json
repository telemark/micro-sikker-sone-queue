{
  "version": 2,
  "name": "micro-sikker-sone-queue",
  "scope": "minelev",
  "alias": [
    "queue.notater.minelev.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_CONNECTION": "@sikker-queue-db",
    "MONGODB_COLLECTION": "@sikker-queue-db-collection",
    "MONGODB_NAME": "@sikker-queue-db-name",
    "JWT_SECRET": "@sikker-queue-secret",
    "PAPERTRAIL_HOSTNAME": "sikker-queue",
    "PAPERTRAIL_HOST": "@tfk-papertrail-host",
    "PAPERTRAIL_PORT": "@tfk-papertrail-port"
  },
  "builds": [
    {
      "src": "/README.md",
      "use": "@now/md"
    },
    {
      "src": "/api/*.js",
      "use": "@now/node"
    },
    {
      "src": "/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    { "src": "/favicon.ico",
      "dest": "/static/favicon.ico"
    },
    {
      "src": "/queue",
      "dest": "/api/queue-add.js"
    },
    {
      "src": "/queue/next",
      "dest": "/api/queue-next.js"
    },
    {
      "src": "/queue/all",
      "dest": "/api/queue-all.js"
    },
    {
      "src": "/queue/(.*)",
      "dest": "/api/queue-delete.js"
    },
    {
      "src": "/(.*)",
      "dest": "/README.html"
    }
  ]
}